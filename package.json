{
  "name": "recipe-app-12",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.local next dev",
    "dev:prod": "env-cmd -f .env.prod next dev",
    "build:local": "env-cmd -f .env.local next build",
    "build:prod": "env-cmd -f .env.prod next build",
    "start:local": "env-cmd -f .env.local next start",
    "start:prod": "env-cmd -f .env.prod next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "start-local-db": "pnpx supabase start --workdir src",
    "stop-local-db": "pnpx supabase stop --workdir src",
    "status-local-db": "env-cmd -f .env.local pnpx supabase status --workdir src",
    "reset-local-db": "env-cmd -f .env.local pnpx supabase db reset --workdir src",
    "reset-db": "env-cmd -f .env.prod pnpx supabase db reset --workdir src",
    "prisma-migrate": "env-cmd -f .env.local pnpx prisma migrate dev",
    "prisma-seed": "pnpx prisma db seed",
    "supabase-types": "pnpx supabase gen --workdir src types typescript --local --schema public > src/types/SupabaseTypes.ts",
    "migrate-and-gen": "env-cmd -f .env.local pnpm run prisma-migrate && pnpm run supabase-types",
    "migrate-and-gen:prod": "env-cmd -f .env.prod pnpm run prisma-migrate && pnpm run supabase-types"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@prisma/client": "4.16.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@supabase/auth-helpers-nextjs": "^0.7.3",
    "@supabase/supabase-js": "^2.26.0",
    "@svgr/webpack": "^8.0.1",
    "@tabler/icons-react": "^2.30.0",
    "@tanstack/react-table": "^8.9.3",
    "@tiptap/extension-image": "^2.1.8",
    "@tiptap/pm": "^2.1.8",
    "@tiptap/react": "^2.1.8",
    "@tiptap/starter-kit": "^2.1.8",
    "@types/node": "20.4.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.8",
    "jotai": "^2.2.2",
    "lucide-react": "^0.258.0",
    "next": "13.4.8",
    "nsfw-filter": "^1.0.3",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-cropper": "^2.3.3",
    "react-day-picker": "^8.8.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.2",
    "react-textarea-autosize": "^8.5.2",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.6",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "^9.0.0",
    "vaul": "^0.3.2",
    "zact": "^0.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/prettier": "^2.7.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/parser": "^5.62.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "4.16.2",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed/index.ts"
  },
  "volta": {
    "node": "18.16.0"
  }
}
